              data-toggle="modal"
              data-target="#primary-certification"
              class="pull-right">
              <i class="fa fa-plus"></i> Add
            </button>
            <div class="col-lg-4">
              <style>
                .center-btn {
                  display: flex;
                  justify-content: center;
                }
              </style>
              <!-- Trigger the modal with a button -->
              <!-- Modal -->
              <div id="primary-certification" class="modal fade" role="dialog">
