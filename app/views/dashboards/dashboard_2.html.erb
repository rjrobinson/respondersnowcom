<div class="wrapper wrapper-content">
  <section>
    <%= render partial: 'responders/responder_metrics' %>
  </section>
  <section>
    <div class="row">
      <div class="col-md-9">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h5 class="panel-title">Search Classes</h5>
          </div>
          <div class="panel-body">
            <div class="input-group stylish-input-group">
              <input type="text" class="form-control" placeholder="Search">
              <span class="input-group-addon">
                <button type="submit">
                  <span class="fas fa-search"></span>
                </button>
              </span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-md-3"></div>
  </section>
  <section>
    <div class="row">
      <div class="col-md-9">
        <div class="ibox float-e-margins">
          <div class="ibox-title">
            <h5>Upcoming
              Classes  <%= "within 10 mi of #{current_responder.zipcode}" if current_responder.zipcode %></h5>
            <div class="ibox-tools">
              <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
              </a>
              <a class="close-link">
                <i class="fa fa-times"></i>
              </a>
            </div>
          </div>
          <div class="ibox-content">
            <table class="table table-hover no-margins">
              <thead>
              <tr>
                <th>Status</th>
                <th>Date</th>
                <th>Class</th>
                <th>Location</th>
                <th></th>
              </tr>
              </thead>
              <tbody>
              <tr>
                <td>
                  <small>open</small>
                </td>
                <td><i class="far fa-clock"></i> 11-10 11:20pm</td>
                <td>CPR</td>
                <td class="text-navy">North Brunswick, NJ</td>
                <td><a href="">register</a></td>
              </tr>
              <tr>
                <td>
                  <small>open</small>
                </td>
                <td><i class="far fa-clock"></i> 11-10 11:20pm</td>
                <td>TCCC</td>
                <td class="text-navy">Camden, NJ</td>
                <td><a href="">register</a></td>
              </tr>
              <tr>
                <td>
                  <small>open</small>
                </td>
                <td><i class="far fa-clock"></i> 11-12 11:20pm</td>
                <td>CPR</td>
                <td class="text-navy">North Brunswick, NJ</td>
                <td><a href="">register</a></td>
              </tr>
              <tr>
                <td>
                  <small>open</small>
                </td>
                <td><i class="far fa-clock"></i> 11-12 11:20pm</td>
                <td>CPR</td>
                <td class="text-navy">North Brunswick, NJ</td>
                <td><a href="">register</a></td>
              </tr>
              <tr>
                <td>
                  <small>open</small>
                </td>
                <td><i class="far fa-clock"></i> 11-12 11:20pm</td>
                <td>First Aid</td>
                <td class="text-navy">North Brunswick, NJ</td>
                <td><a href="">register</a></td>
              </tr>
              <tr>
                <td>
                  <small>open</small>
                </td>
                <td><i class="far fa-clock"></i> 12-12 11:20pm</td>
                <td>Etrication I</td>
                <td class="text-navy">North Brunswick, NJ</td>
                <td><a href="">register</a></td>
              </tr>

              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
    <div class="col-md-3"></div>
</div>
</section>
</div>




