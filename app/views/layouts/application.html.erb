<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="google-site-verification" content="248uddpJf5rTHpjzsMH_C_IMj1DcVjMthvM6PnFpPsI"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>RespondersNOW.com</title>

  <!--FONT AWESOME 5-->
  <script defer src="https://use.fontawesome.com/releases/v5.5.0/js/all.js"></script>
  <%= csrf_meta_tags %>

  <!-- application scss -->
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>

  <!--  google ads -->
  <script async
          src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js">
  </script>

  <script type="text/javascript"
          src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCya6eVkasAwZfvsIovC63YcAc3QcGHZqI&libraries=places">

  </script>

  <!-- Facebook Pixel Code -->
  <script type="text/javascript" charset="utf-8">
      !function (f, b, e, v, n, t, s) {
          if (f.fbq) return;
          n = f.fbq = function () {
              n.callMethod ?
                  n.callMethod.apply(n, arguments) : n.queue.push(arguments);
          };
          if (!f._fbq) f._fbq = n;
          n.push = n;
          n.loaded = !0;
          n.version = '2.0';
          n.queue = [];
          t = b.createElement(e);
          t.async = !0;
          t.src = v;
          s = b.getElementsByTagName(e)[0];
          s.parentNode.insertBefore(t, s);
      }(window, document, 'script',
          'https://connect.facebook.net/en_US/fbevents.js');
      fbq('init', '321900381877398');
      fbq('track', 'PageView');
  </script>
  <noscript>
    <img height="1" width="1" style="display:none"
         src="https://www.facebook.com/tr?id=321900381877398&ev=PageView&noscript=1"/>
  </noscript>
  <!-- End Facebook Pixel Code -->

</head>
<body>
<%= render 'layouts/skinconfig' %>
<div id="wrapper" class="<%= params[:controller] %>.<%= params[:action] %>">
  <%= render 'layouts/navigation' %>
  <div id="page-wrapper" class="gray-bg <%= @extra_class %>">
    <%= render 'layouts/topnavbar' %>
    <%= flash_messages %>
    <div>
      <h1>Responders Now</h1>
    </div>
    <%= yield %>
    <%= render 'layouts/footer' %>
  </div>
</body>
</html>